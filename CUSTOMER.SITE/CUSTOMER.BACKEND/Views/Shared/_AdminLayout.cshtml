<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/Users.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/Shared/logout.js" type="text/javascript"></script>
    <script src="~/Scripts/Shared/jwtParse.js" type="text/javascript"></script>
    <script src="~/Scripts/Adminscripts/navigatorWhenTokenInvalid.js" type="text/javascript"></script>
</head>
<body>

    <div id="header">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-dark">
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="Clients" id="clients">Clients</a>

                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="Admins" id="admins">Admins</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="AddAdmin" id="addAdmin">Add Admin</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" id="logout">Logout</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>
    @RenderBody()
</body>
</html>
